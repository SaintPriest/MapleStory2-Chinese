<?xml version="1.0" encoding="utf-8"?>
	
<ms2>

	<state name="대기">
		<condition name="UserValue" key="Cube" value="1" >	
			<transition state="큐브20"/>
		</condition>
	</state>
	
	<state name="큐브20">
		<onEnter>
			<action name="메쉬를설정한다" arg1="4038-4040" arg2="1" arg3="0" arg4="0" arg5="0" />
			<action name="메쉬를설정한다" arg1="5038-5040" arg2="1" arg3="0" arg4="0" arg5="0" />
		</onEnter>
		<condition name="WaitTick" waitTick="5000" >
			<action name="SetUserValue" triggerID="920020" key="CubeOff" value="1" />
			<transition state="메쉬제거"/>
		</condition>
	</state>
	
	<state name="메쉬제거">
		<onEnter>
		</onEnter>
		<condition name="UserValue" key="Cube" value="2" >
			<action name="SetUserValue" triggerID="910020" key="Cube" value="1" />		
			<transition state="큐브제거"/>
		</condition>
	</state>
	
	<state name="큐브제거">
		<onEnter>
			<action name="메쉬를설정한다" arg1="4038-4040" arg2="0" arg3="0" arg4="0" arg5="0" />
			<action name="메쉬를설정한다" arg1="5038-5040" arg2="0" arg3="0" arg4="0" arg5="0" />
		</onEnter>	
		<condition name="WaitTick" waitTick="4300" >
			<transition state="큐브20"/>
		</condition>
		<condition name="유저를감지했으면" arg1="9004" >
			<action name="SetUserValue" triggerID="910020" key="Cube" value="0" />
			<transition state="종료"/>
		</condition>
		<condition name="유저를감지했으면" arg1="9005" >
			<action name="SetUserValue" triggerID="910020" key="Cube" value="0" />
			<transition state="종료"/>
		</condition>
	</state>
	
	<state name="종료">                   <!--유저가 지나갈 수 있는 메쉬 보여줌-->
		<onEnter>
			<action name="메쉬를설정한다" arg1="4001-4040" arg2="1" arg3="0" arg4="0" arg5="0" />
		</onEnter>
		<condition name="WaitTick" waitTick="2000" >
			<transition state="대기"/>
		</condition>
	</state>
		
</ms2>


			<action name="NPC를이동시킨다" arg1="208" arg2="MS2PatrolData_208New" />			
			<action name="SetUserValue" triggerID="12" key="FindWay" value="1" />		 	
		</onEnter>
				<condition name="WaitTick" waitTick="500">  
					<transition state="Quit02"/>	

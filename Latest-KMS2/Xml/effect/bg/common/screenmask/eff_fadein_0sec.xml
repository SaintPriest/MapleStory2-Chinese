<!-- log  (로그함수)변화값이 점점 낮아짐, exp (지수함수)변화값이 점점 빨라짐 -->
<ms2>
	<effect texture="black.tga" fadeEndDuration="0" holdDuration="" fadeStartDuration="0" fadeStartInterpolator="log" fadeEndInterpolator="log" useFade="true" loop="true" startDelay="0.0" blendingMax="1.0" blendingMin="0.0" type="screenMasking"/>    

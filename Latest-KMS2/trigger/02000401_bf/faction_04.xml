			<action name="오브젝트반응설정한다" arg1="12000040" arg2="2" />					
		</onEnter>
			<condition name="UserValue" key="faction04" value="1">
				<transition state="인원수체크" />
			</condition>	
	</state>
  
	<state name="인원수체크">
		<onEnter>			
			<action name="스킵을설정한다" arg1="반응대기" />		

			<action name="연출UI를설정한다" arg1="3"/>  			
			<action name="몬스터소멸시킨다" arg1="2902" />				
			<!--action name="연출UI를설정한다" arg1="1"/>
			<action name="연출UI를설정한다" arg1="3"/-->  		
			<action name="버프를걸어준다" arg1="199" arg2="70000107" arg3="1" arg4="0" arg5="0" />						
			<action name="카메라를선택한다" arg1="303" arg2="1"/>

			<action name="SetUserValue" triggerID="907" key="MobWaveStart" value="1" /> 		
				</onEnter>		
			<condition name="UserValue" key="07RoundSuccess" value="1">	
			<transition state="Round07_Sucess02"/>	
		</condition> 		
    <onExit> 
    </onExit>	
    </state>	


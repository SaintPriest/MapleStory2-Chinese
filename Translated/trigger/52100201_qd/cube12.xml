<?xml version="1.0" encoding="utf-8"?>
	
<ms2>

	<state name="대기">
		<condition name="UserValue" key="Cube" value="1" >	
			<transition state="큐브12"/>
		</condition>
	</state>
	
	<state name="큐브12">
		<onEnter>
			<action name="메쉬를설정한다" arg1="4020-4021" arg2="1" arg3="0" arg4="0" arg5="0" />
		</onEnter>
		<condition name="WaitTick" waitTick="8000" >
			<transition state="큐브제거"/>
		</condition>
	</state>
	
	<state name="큐브제거">
		<onEnter>
			<action name="메쉬를설정한다" arg1="4020-4021" arg2="0" arg3="0" arg4="0" arg5="0" />
		</onEnter>	
		<condition name="WaitTick" waitTick="3300" >
			<transition state="큐브12"/>
		</condition>
		<condition name="유저를감지했으면" arg1="9004" >
			<action name="SetUserValue" triggerID="910012" key="Cube" value="0" />
			<transition state="종료"/>
		</condition>
		<condition name="유저를감지했으면" arg1="9005" >
			<action name="SetUserValue" triggerID="910012" key="Cube" value="0" />
			<transition state="종료"/>
		</condition>
	</state>
	
	<state name="종료">                   <!--유저가 지나갈 수 있는 메쉬 보여줌-->
		<onEnter>
			<action name="메쉬를설정한다" arg1="4001-4040" arg2="1" arg3="0" arg4="0" arg5="0" />
		</onEnter>
		<condition name="WaitTick" waitTick="2000" >
			<transition state="대기"/>
		</condition>
	</state>
		
</ms2>


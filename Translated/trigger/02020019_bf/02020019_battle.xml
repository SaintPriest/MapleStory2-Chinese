		<condition name="몬스터가죽어있으면" arg1="201" >
			<transition state="전투_1라운드종료"/>
		</condition>
		<condition name="!유저를감지했으면" arg1="901">
			<transition state="전투_종료"/>
		</condition>
		<condition name="DungeonCheckPlayTime" playSeconds="300" >
			<transition state="전투_종료"/>
		</condition>
	</state>


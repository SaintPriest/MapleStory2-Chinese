		</onEnter>
		<condition name="UserValue" key="battlesetting" value="1" >
			<transition state="전투_1라운드세팅"/>
		</condition>
	</state>

	<state name="전투_1라운드세팅"> 
		<onEnter>
			<action name="DungeonResetTime" seconds="300"/>

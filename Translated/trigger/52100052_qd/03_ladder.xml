				</condition>		
		<onExit>
		</onExit>
  </state>
  
  <state name="LadderOn">
		<onEnter>		
			<action name="사다리를설정한다" arg1="501" arg2="1" arg3="1" arg4="2"/>	<!-- Ladder_Shortcut -->

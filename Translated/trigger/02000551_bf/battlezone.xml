<?xml version="1.0" encoding="utf-8"?>
<!-- TriggerModelID = 552, 12시 3시 6시 9시 중앙 넓은 메인 전투판에 보스가 들어서면 보스AI에서 알수 있도록 신호 보내는 기능의 트리거임 -->					
<ms2>

	<state name="시작대기중">
		<condition name="CheckUser">
			<transition state="트리거작동시작" /> 
		</condition>		
	</state> 



	<state name="트리거작동시작"> 
		<onEnter>
		</onEnter>  		
		
			<!--arg2은 몬스터 스폰ID 101는 어려운 난이도 -->
			<condition name="NPC를감지했으면" arg1="13" arg2="101">	<!--중앙 전투판에 블랙빈이 들어셨으면, arg1 은 트리거박스 ID, arg2은 몬스터 스폰ID -->
				<transition state="중앙전투판신호"/>	
			</condition>  

			<condition name="NPC를감지했으면" arg1="12" arg2="101">	<!--12시 전투판에 블랙빈이 들어셨으면, arg1 은 트리거박스 ID, arg2은 몬스터 스폰ID -->
				<transition state="12시전투판신호"/>	
			</condition>  
			
			<condition name="NPC를감지했으면" arg1="3" arg2="101">	<!--3시 전투판에 블랙빈이 들어셨으면, arg1 은 트리거박스 ID, arg2은 몬스터 스폰ID -->
				<transition state="3시전투판신호"/>	
			</condition>  

			<condition name="NPC를감지했으면" arg1="6" arg2="101">	<!--6시 전투판에 블랙빈이 들어셨으면, arg1 은 트리거박스 ID, arg2은 몬스터 스폰ID -->
				<transition state="6시전투판신호"/>	
			</condition>  

			<condition name="NPC를감지했으면" arg1="9" arg2="101">	<!--9시 전투판에 블랙빈이 들어셨으면, arg1 은 트리거박스 ID, arg2은 몬스터 스폰ID -->
				<transition state="9시전투판신호"/>	
			</condition>  			
			



			<condition name="NPC를감지했으면" arg1="122" arg2="101">	<!--봄 컨셉 도로에 블랙빈이 들어셨으면, arg1 은 트리거박스 ID, arg2은 몬스터 스폰ID -->
				<transition state="봄컨셉도로신호"/>	
			</condition>  

			<condition name="NPC를감지했으면" arg1="45" arg2="101">	<!--여름 컨셉 도로에 블랙빈이 들어셨으면, arg1 은 트리거박스 ID, arg2은 몬스터 스폰ID -->
				<transition state="여름컨셉도로신호"/>	
			</condition>  
			
			<condition name="NPC를감지했으면" arg1="78" arg2="101">	<!--가을 컨셉 도로에 블랙빈이 들어셨으면, arg1 은 트리거박스 ID, arg2은 몬스터 스폰ID -->
				<transition state="가을컨셉도로신호"/>	
			</condition>  

			<condition name="NPC를감지했으면" arg1="1011" arg2="101">	<!--겨울 컨셉 도로에 블랙빈이 들어셨으면, arg1 은 트리거박스 ID, arg2은 몬스터 스폰ID -->
				<transition state="겨울컨셉도로신호"/>	
			</condition>  




			
			<!--arg2은 몬스터 스폰ID 102는 쉬운 난이도 -->
			<condition name="NPC를감지했으면" arg1="13" arg2="102">	<!--중앙 전투판에 블랙빈이 들어셨으면, arg1 은 트리거박스 ID, arg2은 몬스터 스폰ID -->
				<transition state="중앙전투판신호"/>	
			</condition>  

			<condition name="NPC를감지했으면" arg1="12" arg2="102">	<!--12시 전투판에 블랙빈이 들어셨으면, arg1 은 트리거박스 ID, arg2은 몬스터 스폰ID -->
				<transition state="12시전투판신호"/>	
			</condition>  
			
			<condition name="NPC를감지했으면" arg1="3" arg2="102">	<!--3시 전투판에 블랙빈이 들어셨으면, arg1 은 트리거박스 ID, arg2은 몬스터 스폰ID -->
				<transition state="3시전투판신호"/>	
			</condition>  

			<condition name="NPC를감지했으면" arg1="6" arg2="102">	<!--6시 전투판에 블랙빈이 들어셨으면, arg1 은 트리거박스 ID, arg2은 몬스터 스폰ID -->
				<transition state="6시전투판신호"/>	
			</condition>  

			<condition name="NPC를감지했으면" arg1="9" arg2="102">	<!--9시 전투판에 블랙빈이 들어셨으면, arg1 은 트리거박스 ID, arg2은 몬스터 스폰ID -->
				<transition state="9시전투판신호"/>	
			</condition>  			
			



			<condition name="NPC를감지했으면" arg1="122" arg2="102">	<!--봄 컨셉 도로에 블랙빈이 들어셨으면, arg1 은 트리거박스 ID, arg2은 몬스터 스폰ID -->
				<transition state="봄컨셉도로신호"/>	
			</condition>  

			<condition name="NPC를감지했으면" arg1="45" arg2="102">	<!--여름 컨셉 도로에 블랙빈이 들어셨으면, arg1 은 트리거박스 ID, arg2은 몬스터 스폰ID -->
				<transition state="여름컨셉도로신호"/>	
			</condition>  
			
			<condition name="NPC를감지했으면" arg1="78" arg2="102">	<!--가을 컨셉 도로에 블랙빈이 들어셨으면, arg1 은 트리거박스 ID, arg2은 몬스터 스폰ID -->
				<transition state="가을컨셉도로신호"/>	
			</condition>  

			<condition name="NPC를감지했으면" arg1="1011" arg2="102">	<!--겨울 컨셉 도로에 블랙빈이 들어셨으면, arg1 은 트리거박스 ID, arg2은 몬스터 스폰ID -->
				<transition state="겨울컨셉도로신호"/>	
			</condition>  
			
			
			<condition name="WaitTick" waitTick="2200">	
				<transition state="시간경과대기"/>
			</condition>	
				
	</state>  

 <state name="시간경과대기">
    <onEnter>
    </onEnter>

		<condition name="WaitTick" waitTick="1500">	
			<transition state="트리거작동시작"/>  <!-- 1~2초 뒤에 다시 처음 단계로 돌아가기-->
		</condition>	
		
  </state>
  

 <state name="중앙전투판신호">
    <onEnter>
		<action name="SetAiExtraData" key="BattleZonePosition" value="13" />	<!-- 보스AI에게 이 변수 13 신호를 보내서 보스가 중앙 전투판에 들어셨음을 알림,  즉 AI_CarParadeBlackbean.xml 혹은 AI_CarParadeBlackbeanBroken.xml 혹은 AI_Blackbean_Phase01.xml 에게 보냄 -->
    </onEnter>

		<condition name="WaitTick" waitTick="5000">	
			<transition state="트리거작동시작"/>  <!-- 변수 신호 보내고 5~6초 뒤에 다시 처음 단계로 돌아가기-->
		</condition>	
		
  </state>
  

<?xml version="1.0" encoding="utf-8"?>
<ms2>

<!-- 플레이어 감지 -->
<!-- 슈팅전 체크 에디셔널 이펙트를 계속 걸어줌 -->

    <state name="IsDungeonRoomReady"> 
	    <onEnter>
		</onEnter>
		<condition name="IsDungeonRoom" > 
			<transition state="idle" />	
		</condition>		
		<condition name="!IsDungeonRoom" > 
			<transition state="questIdle" />	
		</condition>	
		<onExit>
		</onExit>
	</state> 
  
	
  <state name="idle">
    <onEnter>
		<action name="버프를걸어준다" arg1="701" arg2="99910120" arg3="1" arg4="0" arg5="0" />
    </onEnter>
		<condition name="UserValue" key="Ground" value="1">	
			<transition state="ready"/>	
			<action name="버프를삭제한다" arg1="701" arg2="99910120" />
		</condition> 		
		<condition name="WaitTick" waitTick="500">
			<transition state="buff_01"/> 
		</condition>			
		<condition name="UserValue" key="Ending" value="1">	
			<transition state="Ending"/>	
		</condition> 		
    <onExit>
    </onExit>
  </state>
   
  <state name="buff_01">
    <onEnter>
		<action name="버프를걸어준다" arg1="701" arg2="99910120" arg3="1" arg4="0" arg5="0" />
    </onEnter>
		<condition name="UserValue" key="Ground" value="1">	
			<transition state="ready"/>	
			<action name="버프를삭제한다" arg1="701" arg2="99910120" />
		</condition> 		
		<condition name="WaitTick" waitTick="500">
			<transition state="idle"/> 
		</condition>			
		<condition name="UserValue" key="Ending" value="1">	
			<transition state="Ending"/>	
		</condition> 		
    <onExit>
    </onExit>
  </state>
    
  <state name="questIdle">
    <onEnter>
		<action name="버프를걸어준다" arg1="701" arg2="99910120" arg3="1" arg4="0" arg5="0" />
    </onEnter>
		<condition name="퀘스트유저를감지하면" arg1="701" arg2="50001518" arg3="1">
			<transition state="Ending"/>
		</condition>	
		<condition name="퀘스트유저를감지하면" arg1="701" arg2="50001517" arg3="2">
			<transition state="Ending"/>
		</condition>	
		<condition name="UserValue" key="Ground" value="1">	
			<transition state="ready"/>	
			<action name="버프를삭제한다" arg1="701" arg2="99910120" />
		</condition> 		
		<condition name="WaitTick" waitTick="500">
			<transition state="questIdle_buff_01"/> 
		</condition>			
		<condition name="UserValue" key="Ending" value="1">	
			<transition state="Ending"/>	
		</condition> 		
    <onExit>
    </onExit>
  </state>
   
  <state name="questIdle_buff_01">
    <onEnter>
		<action name="버프를걸어준다" arg1="701" arg2="99910120" arg3="1" arg4="0" arg5="0" />
    </onEnter>
		<condition name="퀘스트유저를감지하면" arg1="701" arg2="50001518" arg3="1">
			<transition state="Ending"/>
		</condition>	
		<condition name="UserValue" key="Ground" value="1">	
			<transition state="ready"/>	
			<action name="버프를삭제한다" arg1="701" arg2="99910120" />
		</condition> 		
		<condition name="WaitTick" waitTick="500">
			<transition state="questIdle"/> 
		</condition>			
		<condition name="UserValue" key="Ending" value="1">	
			<transition state="Ending"/>	
		</condition> 		
    <onExit>
    </onExit>
  </state>	
	
  
  <state name="ready">
    <onEnter>
		<action name="EnableSpawnPointPC" spawnPointID="11001" isEnable="0" />
		<action name="EnableSpawnPointPC" spawnPointID="11002" isEnable="1" />
		<action name="버프를삭제한다" arg1="701" arg2="99910120" />
		<action name="메쉬를설정한다" arg1="1001-1002" arg2="0" />
		<action name="메쉬를설정한다" arg1="1004-1006" arg2="0" />
<!-- 		<action name="SetLocalCamera" cameraId="8001" enable="0"/> -->
 		<action name="SetLocalCamera" cameraId="8002" enable="1"/>	
		<action name="대화를설정한다" arg1="1" arg2="102" arg3="$02000390_BF__AI__0$" arg4="2" arg5="0"/>
		<action name="대화를설정한다" arg1="1" arg2="101" arg3="$02000390_BF__AI__1$" arg4="2" arg5="2"/>
		<action name="몬스터소멸시킨다" arg1="501,502,503,504,505,506,507,508,509,510"/> <!-- 수중 위 몬스터 제거 -->
    </onEnter>
		<condition name="UserValue" key="Ending" value="1">	
			<transition state="Ending"/>	
		</condition> 	
		<condition name="몬스터가죽어있으면" arg1="201,210">
            <transition state="Ending" />
        </condition>		
    <onExit>
    </onExit>
  </state>

  
    <state name="Ending">
    <onEnter>
		<action name="버프를삭제한다" arg1="701" arg2="99910120" />
		<action name="이펙트를설정한다" arg1="7001" arg2="1"/>
		<action name="SetOnetimeEffect" id="1" enable="1" path="BG/Common/ScreenMask/Eff_fadein_1sec.xml"/>		
		<action name="몬스터소멸시킨다" arg1="201,210,101,102"/> <!-- 메비딕 제거 -->
		<action name="몬스터를생성한다" arg1="202,103,104" arg2="1" />
		<action name="연출UI를설정한다" arg1="1"/> 
		<action name="연출UI를설정한다" arg1="3"/> 
    </onEnter>
		<condition name="WaitTick" waitTick="2000">
			<transition state="Ending_02"/> 
		</condition>	
    <onExit>
    </onExit>
	
  </state>
  

   <state name="Ending_02">
    <onEnter>
		<action name="스킵을설정한다" arg1="Ending_04" />
		<action name="몬스터소멸시킨다" arg1="501,502,503,504,505,506,507,508,509,510"/> <!-- 수중 위 몬스터 제거 -->
		<action name="카메라경로를선택한다" arg1="8101,8102,8103" arg2="0"/>
		<action name="SetOnetimeEffect" id="1" enable="0" path="BG/Common/ScreenMask/Eff_fadein_1sec.xml"/>	
		<action name="SetNpcEmotionloop" arg1="202" arg2="Stun_A" arg3="9000000"/>	
		<action name="NPC를이동시킨다" arg1="103" arg2="MS2PatrolData_2008" />	
		<action name="NPC를이동시킨다" arg1="104" arg2="MS2PatrolData_2007" />			
		<action name="대화를설정한다" arg1="1" arg2="103" arg3="$02000390_BF__AI__2$" arg4="2" arg5="0"/>
		<action name="대화를설정한다" arg1="1" arg2="104" arg3="$02000390_BF__AI__3$" arg4="2" arg5="1"/>
	</onEnter>
		<condition name="WaitTick" waitTick="3000">
			<transition state="Ending_03"/> 
		</condition>
    <onExit>
    </onExit>
  </state>  
     
  <state name="Ending_03">
    <onEnter>
		<action name="대화를설정한다" arg1="1" arg2="202" arg3="$02000390_BF__AI__4$" arg4="2" arg5="0"/>
		<action name="대화를설정한다" arg1="1" arg2="104" arg3="$02000390_BF__AI__5$" arg4="2" arg5="2"/>
		<action name="대화를설정한다" arg1="1" arg2="103" arg3="$02000390_BF__AI__6$" arg4="2" arg5="3"/>
		<action name="대화를설정한다" arg1="1" arg2="202" arg3="$02000390_BF__AI__7$" arg4="2" arg5="6"/>
	</onEnter>
		<condition name="WaitTick" waitTick="8000">
			<transition state="Ending_04"/> 
		</condition>
    <onExit>
    </onExit>
  </state>
  
   <state name="Ending_04">
    <onEnter>
		<action name="SetOnetimeEffect" id="1" enable="1" path="BG/Common/ScreenMask/Eff_fadein_1sec.xml"/>
	</onEnter>
		<condition name="WaitTick" waitTick="1000">
			<transition state="Ending_04_b"/> 
		</condition>
    <onExit>
    </onExit>
  </state>
  
     <state name="Ending_04_b">
    <onEnter>
  		<action name="카메라리셋" interpolationTime="0.0"/>	 
	</onEnter>
		<condition name="WaitTick" waitTick="2000">
			<transition state="Ending_05"/> 
		</condition>
    <onExit>
    </onExit>
  </state>
 
  <state name="Ending_05">
    <onEnter>
		<action name="SetLocalCamera" cameraId="8001" enable="0"/>		<!-- LocalTargetCamera -->	
		<action name="SetLocalCamera" cameraId="8002" enable="0"/>		<!-- LocalTargetCamera -->	
		<action name="연출UI를설정한다" arg1="0"/> 
		<action name="연출UI를설정한다" arg1="2"/> 
		<action name="SetOnetimeEffect" id="1" enable="0" path="BG/Common/ScreenMask/Eff_fadein_1sec.xml"/>	
	</onEnter>
		<condition name="WaitTick" waitTick="2000">
			<transition state="IsDungeonRoom"/> 
		</condition>
    <onExit>
    </onExit>
  </state>
  
    <state name="IsDungeonRoom"> 
	    <onEnter>
		</onEnter>
		<condition name="IsDungeonRoom" > 
			<transition state="dungeonEnd" />	
		</condition>		
		<condition name="!IsDungeonRoom" > 
			<transition state="questEnd" />	
		</condition>	
		<onExit>
		</onExit>
	</state> 
  
  
  <state name="dungeonEnd">
    <onEnter>
		<action name="메쉬를설정한다" arg1="1004-1006" arg2="0" />
		<action name="이펙트를설정한다" arg1="7001" arg2="0"/>
		<action name="메쉬를설정한다" arg1="1001-1002" arg2="0" />
		<action name="포탈을설정한다" arg1="1" arg2="1" arg3="1" arg4="1"/>
		<action name="업적이벤트를발생시킨다" arg1="701" arg2="trigger" arg3="clearalbanos"/> 
		<action name="업적이벤트를발생시킨다" arg1="701" arg2="trigger" arg3="ClearOceanKing"/>
		<action name="DungeonClear" />
		
	</onEnter>
		<condition name="퀘스트유저를감지하면" arg1="701" arg2="50001518" arg3="1">
			<transition state="QuestEnd_warp"/>
		</condition>
    <onExit>
    </onExit>
  </state>
  
    <state name="questEnd">
    <onEnter>
		<action name="버프를삭제한다" arg1="701" arg2="99910120" />
		<action name="메쉬를설정한다" arg1="1004-1006" arg2="0" />
		<action name="이펙트를설정한다" arg1="7001" arg2="0"/>
		<action name="메쉬를설정한다" arg1="1001-1002" arg2="0" />
		<action name="업적이벤트를발생시킨다" arg1="701" arg2="trigger" arg3="clearalbanos"/>
	</onEnter>
		<condition name="퀘스트유저를감지하면" arg1="701" arg2="50001518" arg3="1">
			<transition state="QuestEnd_warp"/>
		</condition>		
    <onExit>
    </onExit>
  </state>
  
  <state name="QuestEnd_warp">
    <onEnter>	
    </onEnter>
		<condition name="WaitTick" waitTick="2000">
			<transition state="QuestEnd_warp_End"/> 
		</condition>
    <onExit>
		<action name="유저를이동시킨다" arg1="52000097" arg2="0"/>
    </onExit>
  </state> 

    <state name="QuestEnd_warp_End">
    <onEnter>	
    </onEnter>
    <onExit>
    </onExit>
  </state> 
</ms2>

